<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="mainwindow">
    <property name="cursor">arrow</property>
    <property name="height">600</property>
    <property name="padding">20</property>
    <property name="takefocus">true</property>
    <property name="width">600</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <property name="sticky">nesw</property>
    </layout>
    <child>
      <object class="ttk.Button" id="btnGetDescription">
        <property name="command">FindDesc</property>
        <property name="default">normal</property>
        <property name="text" translatable="yes">Find From Code</property>

        <layout>
          <property name="column">0</property>
          <property name="ipadx">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="tboxCodeEntry">
        <property name="font">TkDefaultFont</property>
        <property name="width">50</property>
        <layout>
          <property name="column">0</property>
          <property name="padx">1</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="Scan XML">
        <property name="command">ScanXML</property>
        <property name="text" translatable="yes">Find GTIP XMLs</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="lCode">
        <property name="text" translatable="yes"> </property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">False</property>
          <property name="row">5</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="btnGetCode">
        <property name="command">FindCode</property>
        <property name="default">normal</property>
        <property name="text" translatable="yes">Find From Description</property>

        <layout>
          <property name="column">0</property>
          <property name="ipadx">0</property>
          <property name="padx">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="tboxDescEntry">
        <property name="font">TkDefaultFont</property>
        <property name="width">50</property>
        <layout>
          <property name="column">0</property>
          <property name="padx">1</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="tboxDir">
        <property name="text" translatable="yes">D:\Projects\jguar_GIT_Set\jprod</property>
        <property name="width">50</property>
        <layout>
          <property name="column">0</property>
          <property name="padx">1</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="tInfo">
        <property name="autoseparators">false</property>
        <property name="blockcursor">false</property>
        <property name="height">1</property>
        <property name="state">normal</property>
        <property name="width">60</property>
        <property name="wrap">none</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">8</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="tInfoCode">
        <property name="height">1</property>
        <property name="state">normal</property>
        <property name="width">60</property>
        <property name="wrap">none</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">9</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="tInfoDir">
        <property name="height">1</property>
        <property name="state">normal</property>
        <property name="width">60</property>
        <property name="wrap">none</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">10</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
